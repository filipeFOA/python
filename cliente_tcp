import socket

ip = "192.168.1.73"
port = 443

try:
    s = socket.socket(socket.AF_INET, socket.SOCK_STREAM)
    s.connect((ip, port))
    msg = ""
    while str(msg[:-1]) != "quit":
        msg = raw_input("Digite uma mensagem: ")
        s.send(msg)
        recebe = s.recv(1024)
        print str(recebe)
    s.close()
except Exception as erro:
    print erro
